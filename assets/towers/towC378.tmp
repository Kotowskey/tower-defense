<svg xmlns="http://www.w3.org/2000/svg" viewBox="0 0 200 160">
  <!-- Baza pod wieże -->
  <g transform="translate(100, 80)">
    <!-- Warstwa dolna -->
    <rect x="-60" y="30" width="120" height="20" fill="#555555" stroke="#222222" stroke-width="2" rx="5" ry="5"/>
    
    <!-- Warstwa główna -->
    <rect x="-50" y="0" width="100" height="30" fill="#777777" stroke="#333333" stroke-width="2" rx="3" ry="3"/>
    
    <!-- Elementy wewnętrzne -->
    <rect x="-40" y="10" width="80" height="10" fill="#999999" stroke="#555555" stroke-width="1"/>
    
    <!-- Ozdobne pręty -->
    <rect x="-45" y="-10" width="10" height="10" fill="#888888" stroke="#444444" stroke-width="1"/>
    <rect x="35" y="-10" width="10" height="10" fill="#888888" stroke="#444444" stroke-width="1"/>
    
    <!-- Gniazdo montażowe -->
    <circle cx="0" cy="0" r="20" fill="#666666" stroke="#333333" stroke-width="2"/>
    <circle cx="0" cy="0" r="15" fill="#444444" stroke="#222222" stroke-width="1"/>
    
    <!-- Elementy ozdobne -->
    <circle cx="-30" cy="15" r="5" fill="#aaaaaa" stroke="#777777" stroke-width="1"/>
    <circle cx="30" cy="15" r="5" fill="#aaaaaa" stroke="#777777" stroke-width="1"/>
    
    <!-- Wzór rozchodzących się linii -->
    <line x1="-15" y1="0" x2="-5" y2="0" stroke="#888888" stroke-width="2"/>
    <line x1="15" y1="0" x2="5" y2="0" stroke="#888888" stroke-width="2"/>
    <line x1="0" y1="-15" x2="0" y2="-5" stroke="#888888" stroke-width="2"/>
    <line x1="0" y1="15" x2="0" y2="5" stroke="#888888" stroke-width="2"/>
  </g>
</svg>
